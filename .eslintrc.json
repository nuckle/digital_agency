{
    "env": {
        "es2021": true,
        "node": true,
        "jquery": true,
        "browser": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "ignorePatterns": ["gulpfile.js"],
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "one-var": ["error", "never"],
        "no-unused-vars": "off"
    }
}
